    <script src="~/assets/vendor_components/jspdf/jspdf.min.js" ></script>
    <script src="~/assets/vendor_components/jspdf/jspdf.plugin.autotable.js" ></script>
    <script src="~/assets/vendor_components/jspdf/jspdf.plugin.autotable.min.js" ></script>
    <script src="~/assets/vendor_components/jspf/jspdf.min.js"></script>
